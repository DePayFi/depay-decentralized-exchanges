  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DePayFi/depay-web3-exchanges/issues"
  },
  "homepage": "https://github.com/DePayFi/depay-web3-exchanges#readme",
  "private": false,
  "dependencies": {
    "depay-web3-client": "^3.0.0",
    "depay-web3-constants": "^2.0.0",
    "depay-web3-tokens": "^2.0.0",
    "depay-web3-transaction": "^2.0.0"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@rollup/plugin-commonjs": "^18.0.0",
